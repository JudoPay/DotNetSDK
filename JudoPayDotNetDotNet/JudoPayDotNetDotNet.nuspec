<?xml version="1.0"?>
<package >
    <metadata>
        <id>JudoPay.Net</id>
        <version>$version$</version>
        <title>JudoPay.Net</title>
        <authors>JudoPay</authors>
        <owners>JudoPay</owners>
        <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
        <projectUrl>https://github.com/JudoPay/DotNetSDK</projectUrl>
